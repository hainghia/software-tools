{
  "manifest_version": 2,
  "update_url":"http://clients2.google.com/service/update2/crx",
  "name": "GetPaidStock",
  "version": "1.0",
  "description": "Search 30+ sites with GetPaidStock.com!",

  "icons": {
    "16": "img/logo16.png",
    "48": "img/logo48.png",
    "128": "img/logo128.png"
  },
  
  "browser_action": {
	"default_title": "GetPaidStock",
    "default_icon": "img/logo19.png",
    "default_popup": "popup.html"
  },
  
  "content_scripts":[
	{
		"matches": [ "http://*/*", "https://*/*" ],
		"js":["init.js", "jquery.min.js", "google-analytics.js"],
           "all_frames": true
	}
  ],
  
  "permissions": [
    "http://*/*",
    "https://*/*",
    "webRequest",
    "tabs"
  ]
}